{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["@nuxtjs/eslint-config-typescript", "plugin:vue/vue3-recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "vue/multi-word-component-names": "off",
    "comma-dangle": 0
  }
}
